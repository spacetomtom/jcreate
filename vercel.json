    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/server.js"
    },
     {"source":"/channel", "destination": "/channels"}
  ],
  "redirects": [
    {"source":"/channel", "destination": "/channels"}
  ]
}
