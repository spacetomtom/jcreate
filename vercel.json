{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/server.js"
    }
  ]
  "routes": [
  {  "src":"/channels/(?<id>[^/]*/check-processing)","dest":"/channels?link=$id"}
  ]
}
