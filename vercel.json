{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/server.js"
    }
  ],
  "routes": [
  {  
  "src":"/channel/(?<product>[^/]+)",
  "dest":"/channels?link=$product"
},
{ "src": "/channel", "dest": "/channels" },
  ]
}
